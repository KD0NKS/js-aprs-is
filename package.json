{
  "name": "js-aprs-is",
  "version": "0.0.1",
  "description": "NodeJs library for connecting to an APRS-IS server.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "setup": "npm install",
    "test": "npm run build && nyc --reporter=text --reporter=lcov mocha dist/test/**/*.test.js",
    "test-only": "mocha dist/test/**/*.test.js",
    "test-report": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "tsc": "./node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KD0NKS/js-aprs-is.git"
  },
  "keywords": [
    "APRS",
    "APRS-IS",
    "APRSC",
    "ham radio",
    "amateur radio"
  ],
  "author": "afairhurst",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/node": "^12.11.0"
  }
}
